@article{Bush2020,
	title = {Epidemiology of ฮฒ-{Lactamase}-{Producing} {Pathogens}},
	volume = {33},
	issn = {1098-6618},
	doi = {10.1128/CMR.00047-19},
	abstract = {ฮฒ-Lactam antibiotics have been widely used as therapeutic agents for the past 70โyears, resulting in emergence of an abundance of ฮฒ-lactam-inactivating ฮฒ-lactamases. Although penicillinases in Staphylococcus aureus challenged the initial uses of penicillin, ฮฒ-lactamases are most important in Gram-negative bacteria, particularly in enteric and nonfermentative pathogens, where collectively they confer resistance to all ฮฒ-lactam-containing antibiotics. Critical ฮฒ-lactamases are those enzymes whose genes are encoded on mobile elements that are transferable among species. Major ฮฒ-lactamase families include plasmid-mediated extended-spectrum ฮฒ-lactamases (ESBLs), AmpC cephalosporinases, and carbapenemases now appearing globally, with geographic preferences for specific variants. CTX-M enzymes include the most common ESBLs that are prevalent in all areas of the world. In contrast, KPC serine carbapenemases are present more frequently in the Americas, the Mediterranean countries, and China, whereas NDM metallo-ฮฒ-lactamases are more prevalent in the Indian subcontinent and Eastern Europe. As selective pressure from ฮฒ-lactam use continues, multiple ฮฒ-lactamases per organism are increasingly common, including pathogens carrying three different carbapenemase genes. These organisms may be spread throughout health care facilities as well as in the community, warranting close attention to increased infection control measures and stewardship of the ฮฒ-lactam-containing drugs in an effort to control selection of even more deleterious pathogens.},
	language = {eng},
	number = {2},
	journal = {Clinical Microbiology Reviews},
	author = {Bush, Karen and Bradford, Patricia A.},
	
	year = {2020},
	pmid = {32102899},
	pmcid = {PMC7048014},
	keywords = {Animals, Anti-Bacterial Agents, Bacteria, Bacterial Proteins, Humans, epidemiology, Plasmids, Drug Resistance, Multiple, Bacterial, beta-Lactamases, beta-Lactams, Bacterial Infections, resistance, ESBL, beta-lactamase, carbapenemase, Clinical Laboratory Techniques},
