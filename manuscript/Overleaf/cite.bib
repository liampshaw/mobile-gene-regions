}

@article{Kikuchi2022,
	title = {Landscape of bla$_{\textrm{{NDM}}}$ genes in {Enterobacteriaceae}},
	volume = {75},
	issn = {1881-1469},
	doi = {10.1038/s41429-022-00553-3},
	abstract = {The blaNDM-1 gene encodes a carbapenemase, New Delhi metallo-ฮฒ-lactamase (NDM-1), and the ability to produce NDM-1 is spread among Enterobacteriaceae via horizontal gene transfer of plasmids. It has been widely accepted that bla$_{\textrm{NDM-1}}$ is regulated by a hybrid promoter (P$_{\textrm{ISAba125}}$) consisting of a -10 box from the original bla$_{\textrm{NDM-1}}$ and a -35 box from ISAba125. However, the conservation of this promoter and the vertical transmission of bla$_{\textrm{NDM}}$ genes by chromosomal integration have not been comprehensively analyzed. We retrieved the region containing the ORF of bla$_{\textrm{NDM-1}}$ (\&gt;95\% translated protein identity) and a region 120โbp upstream of the bla$_{\textrm{NDM-1}}$ start codon from the complete sequence data of Enterobacteriaceae plasmids (nโ=โ10,914) and chromosomes (nโ=โ4908) deposited in GenBank, and the 310 extracted bla$_{\textrm{NDM}}$ genes were analyzed by an in-silico approach. The results showed that most bla$_{\textrm{NDM}}$ genes (99.0\%) utilized the promoter, P$_{\textrm{ISAba125}}$. Interestingly, two bla$_{\textrm{NDM-1}}$ genes from the genus Citrobacter utilized the ISCR1-derived outward-oriented promoters P$_{\textrm{OUT}}$ (P$_{\textrm{ISCR1}}$). Furthermore, the insertion of ISAba125 and ISCR1 occurred upstream of the CCATATTT sequence, which is located upstream of the -10 box. We also confirmed that most of the bla$_{\textrm{NDM}}$ genes were disseminated by horizontal gene transfer of the plasmid, but 10 cases of the bla$_{\textrm{NDM}}$ genes were integrated into the chromosome via mobile genetic elements such as IS26, IS150, ISCR1, ICE, and Tn7-like elements. Thus, plasmid-mediated transmission of the P$_{\textrm{ISAba125}}$-bla$_{\textrm{NDM}}$ genes is predominant in Enterobacteriaceae. However, the spread of bla$_{\textrm{NDM}}$ genes with new promoters and vertical dissemination via chromosomal integrations may pose additional serious clinical problems.},
	language = {eng},
	number = {10},
	journal = {The Journal of Antibiotics},
	author = {Kikuchi, Yuta and Matsui, Hidehito and Asami, Yukihiro and Kuwae, Asaomi and Inahashi, Yuki and Hanaki, Hideaki and Abe, Akio},
	
	year = {2022},
	pmid = {35986092},
	keywords = {Anti-Bacterial Agents, Plasmids, Gene Transfer, Horizontal, Enterobacteriaceae, Microbial Sensitivity Tests, beta-Lactamases},
	pages = {559--566},
}

